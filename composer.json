{
	"name": "t3docs/codesnippet",
	"description": "This extension packages creates restructured files to document the API",
	"license": [
		"GPL-2.0+"
	],
	"type": "typo3-cms-extension",
	"authors": [
		{
			"name": "Documentation Team",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^11.5 || ^12.0 || dev-main"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.28",
		"typo3/coding-standards": "^0.5.5",
		"typo3/testing-framework": "~7.0@dev"
	},
	"autoload": {
		"psr-4": {
			"T3docs\\Codesnippet\\": "Classes/"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"ergebnis/composer-normalize": true,
			"sbuerk/typo3-cmscomposerinstallers-testingframework-bridge": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "12.0.x-dev"
		},
		"typo3/cms": {
			"extension-key": "codesnippet",
			"web-dir": ".Build/Web"
		}
	},
	"scripts": {
		"prepare-release": [
			"rm -rf .github",
			"rm .gitignore",
			"rm .editorconfig"
		]
	}
}
